{
  "name": "fragments-ui",
  "private": true,
  "version": "0.0.1",
  "description": "Fragments UI testing web app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel src/index.html --port 1234",
    "build": "parcel build src/index.html",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"**/*.js\" \"src/**/*.html\" README.md",
    "format:check": "prettier --check \"**/*.js\" \"src/**/*.html\" README.md",
    "test:smoke": "node ./tests/smoke.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harshpahurkar/fragments-ui.git"
  },
  "author": "Harsh Pahurkar",
  "license": "UNLICENSED",
  "dependencies": {
    "dotenv": "^17.2.2",
    "oidc-client-ts": "^3.3.0"
  },
  "devDependencies": {
    "parcel": "^2.15.4",
    "eslint": "^8.45.0",
    "prettier": "^2.8.8",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.html": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/harshpahurkar/fragments-ui/issues"
  },
  "homepage": "https://github.com/harshpahurkar/fragments-ui#readme"
}
